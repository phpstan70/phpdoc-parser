{
	"name": "phpstan70/phpdoc-parser",
	"description": "PHP 7.0 support for: PHPDoc parser with support for nullable, intersection and generic types",
	"license": "MIT",
	"require": {
		"php": "^7.0.0"
	},
	"require-dev": {
		"php-parallel-lint/php-parallel-lint": "^1.2",
		"phpunit/phpunit": "*",
		"symfony/process": "*"
	},
	"config": {
		"platform": {
			"php": "7.0.0"
		},
		"sort-packages": true,
		"allow-plugins": {
			"phpstan/extension-installer": true
		}
	},
	"autoload": {
		"psr-4": {
			"PHPStan\\PhpDocParser\\": [
				"src/"
			]
		}
	},
	"autoload-dev": {
		"psr-4": {
			"PHPStan\\PhpDocParser\\": [
				"tests/PHPStan"
			]
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"replace": {
		"phpstan/phpdoc-parser": "*"
	}
}
